{
  "name": "valid-me-react",
  "version": "1.0.1",
  "files": "./src/**/*.js",
  "description": "Kit created to easily add validation to inputs in your page, live demo : https://magestican.github.io/valid-me-react/",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html && npm run lint && npm run test",
    "build": "npm run open:dist",
    "build-web": "babel-node tools/build.js && babel-node tools/buildHtml.js",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "prepublish": "npm run build-web"
  },
  "main": "./index.js",
  "author": "Bryan Arbelo",
  "license": "MIT",
  "dependencies": {
    "google-libphonenumber": "3.0.0",
    "react": "15.0.2",
    "react-dom": "15.0.2"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-node6": "11.0.0",
    "amd-define-factory-patcher-loader": "1.0.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.26.0",
    "browser-sync": "2.12.5",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.20.0",
    "connect-history-api-fallback": "1.2.0",
    "coveralls": "2.11.9",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "es6-promise": "3.2.1",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "8.0.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-jsx-a11y": "1.0.4",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "expose-loader": "0.7.1",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.8.5",
    "isparta": "4.0.0",
    "jquery": "3.1.0",
    "mocha": "2.4.5",
    "node-sass": "4.5.3",
    "npm-run-all": "1.8.0",
    "object-assign": "4.1.0",
    "react-addons-test-utils": "15.0.2",
    "react-hot-loader": "1.3.0",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "redux": "3.5.2",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.1",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "3.5.5",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.10.0",
    "whatwg-fetch": "1.0.0"
  },
  "keywords:": [
    "react",
    "reactjs",
    "react-router",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "redux",
    "flux",
    "boilerplate",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/magestican/valid-me-react"
  }
}
